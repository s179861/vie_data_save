CREATE TABLE Person (
id INTEGER AUTO INCREMENT,
name VARCHAR(30),
additionalName VARCHAR(30),
description TEXT,
address VARCHAR(50),
birthDate DATE,
birthPlace VARCHAR(30),
deathDate DATE,
deathPlace VARCHAR(30),
email VARCHAR(30),
familyName VARCHAR(30),
faxNumber VARCHAR(10),
gender VARCHAR(6)
height INTEGER,
nationality VARCHAR(20),
telephone VARCHAR(12),
weight INTEGER,
PRIMARY KEY id
);
CREATE TABLE Person ( id INTEGER AUTO INCREMENT, name VARCHAR(30), additionalName VARCHAR(30), description TEXT, address VARCHAR(50), birthDate DATE, birthPlace VARCHAR(30), deathDate DATE, deathPlace VARCHAR(30), email VARCHAR(30), familyName VARCHAR(30), faxNumber VARCHAR(10), gender VARCHAR(6) height INTEGER, nationality VARCHAR(20), telephone VARCHAR(12), weight INTEGER, PRIMARY KEY id );
